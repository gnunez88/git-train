# ${HOME}/.ssh/config
# git clone git@gitlab.com:<username>/<repo>.git
Host gitlab
    Hostname gitlab.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitlab
# git clone git@github.com:<username>/<repo>.git
Host github
    Hostname github.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/github
